{
  "name": "legend-builder",
  "version": "1.0.0-alpha.8",
  "description": "An advanced League Of Legends champion builder",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "homepage": "http://legendbuilder.io/",
  "scripts": {
    "check": "./node_modules/.bin/depcheck --parsers'*.js:es6,*.ts:typescript' --specials=bin,eslint,webpack",
    "preclean": "./node_modules/.bin/rimraf typings coverage dist node_modules",
    "clean": "npm cache clean",
    "clean:dist:client": "./node_modules/.bin/rimraf dist/client",
    "clean:dist:server": "./node_modules/.bin/rimraf dist/server",
    "clean:dist:spec": "./node_modules/.bin/rimraf dist/spec",
    "coveralls": "./node_modules/.bin/coveralls < ./coverage/lcov/lcov.info",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm run build:client:prod",
    "prebuild:client:dev": "npm run clean:dist:client && npm run lint:client",
    "build:client:dev": "./node_modules/.bin/webpack --config config/webpack/client/dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:client:prod": "npm run clean:dist:client && npm run lint:client",
    "build:client:prod": "./node_modules/.bin/webpack --config config/webpack/client/prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "build:server": "npm run build:server:prod",
    "prebuild:server:dev": "npm run clean:dist:server && npm run lint:server",
    "build:server:dev": "./node_modules/.bin/webpack --config config/webpack/server/dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:server:prod": "npm run clean:dist:server && npm run lint:server",
    "build:server:prod": "./node_modules/.bin/webpack --config config/webpack/server/prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "prebuild:server:test": "npm run clean:dist:spec && npm run lint:server:test",
    "build:server:test": "./node_modules/.bin/webpack --config config/webpack/server/test.js --progress --profile --colors --display-error-details --display-cached",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "start": "npm run http-server",
    "server": "npm run http-server",
    "http-server": "npm run http-server:dev",
    "http-server:dev": "./node_modules/.bin/webpack-dev-server --config config/webpack/client/dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "http-server:prod": "node dist/server/http-server.js",
    "static-server": "npm run build:server:dev && node dist/server/static-server.js",
    "match-server": "npm run build:server:dev && node dist/server/match-server.js",
    "clang-format": "./node_modules/.bin/clang-format",
    "format": "npm run clang-format -- --glob=\"@(src|config)/**/*.@(ts|js)\" -i",
    "format:client": "npm run clang-format -- --glob=\"src/client/**/*.@(ts|js)\" -i",
    "format:server": "npm run clang-format -- --glob=\"src/server/**/*.@(ts|js)\" -i",
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "lint:client": "./node_modules/.bin/tslint src/client/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts -e **/testing/**",
    "lint:server": "./node_modules/.bin/tslint src/server/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts -e **/testing/**",
    "lint:client:test": "./node_modules/.bin/tslint src/client/**/*.spec.ts src/client/**/testing/**/*.ts",
    "lint:server:test": "./node_modules/.bin/tslint src/server/**/*.spec.ts src/server/**/testing/**/*.ts",
    "lint:test": "./node_modules/.bin/tslint src/**/*.spec.ts src/**/testing/**/*.ts",
    "lint:e2e": "./node_modules/.bin/tslint src/**/*.e2e.ts",
    "pree2e": "npm run lint:e2e && npm run webdriver:update -- --standalone",
    "e2e": "./node_modules/.bin/protractor config/protractor.conf.js",
    "e2e:live": "./node_modules/.bin/protractor config/protractor.conf.js --elementExplorer",
    "pretest": "npm run lint:test",
    "test": "npm run test:client && npm run test:server",
    "test:client": "./node_modules/.bin/karma start config/karma.conf.js",
    "pretest:server": "npm run build:server:test",
    "test:server": "node config/jasmine.js",
    "ci": "npm run e2e && npm run test",
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install",
    "reddit-release": "python src/reddit/release.py",
    "release": "./node_modules/.bin/standard-version && npm run reddit-release"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "async": "1.5.2",
    "chalk": "1.1.3",
    "core-js": "2.4.0",
    "d3-axis": "1.0.0",
    "d3-scale": "1.0.0",
    "d3-selection": "1.0.0",
    "d3-shape": "1.0.0",
    "dateformat": "1.0.12",
    "es6-shim": "0.35.1",
    "lru-cache": "4.0.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^1.1.1",
    "clang-format": "^1.0.42",
    "codelyzer": "^0.0.24",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "depcheck": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.14.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jasmine-matchers-loader": "^0.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.6",
    "protractor": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "standard-version": "^2.2.1",
    "style-loader": "^0.13.1",
    "tslint": "^3.12.1",
    "tslint-loader": "^2.1.3",
    "typings": "^1.0.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0"
  }
}
