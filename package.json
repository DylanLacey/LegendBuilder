{
  "name": "legend-builder",
  "version": "1.0.0-alpha.5",
  "description": "An advanced League Of Legends champion builder.",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist": "rimraf dist",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:prod": "http-server dist --cors",
    "static-server": "node src/server/staticServer.js",
    "match-server": "node src/server/matchServer.js",
    "lint": "./node_modules/.bin/tslint src/**/*.ts -e src/typings/",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.9",
    "core-js": "^2.2.0",
    "d3": "^3.5.12",
    "lru-cache": "2.7.0",
    "rxjs": "^5.0.0-beta.2",
    "tinytim": "^0.1.1",
    "tracer": "0.8.2",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "awesome-typescript-loader": "^0.16.0-rc.0",
    "conventional-github-releaser": "^0.5.1",
    "copy-webpack-plugin": "^1.1.1",
    "del": "^2.2.0",
    "gulp": "^3.9.0",
    "gulp-bump": "^1.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-conventional-changelog": "^0.7.0",
    "gulp-file": "^0.2.0",
    "gulp-flatten": "^0.2.0",
    "gulp-git": "^1.6.0",
    "gulp-minify-css": "^1.2.3",
    "gulp-rename": "^1.2.2",
    "gulp-shell": "^0.5.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.10.0",
    "gulp-uglify": "^1.5.1",
    "gulp-uglifycss": "^1.0.5",
    "gulp-util": "^3.0.7",
    "gulp-webpack": "^1.5.0",
    "html-webpack-plugin": "^2.10.0",
    "live-server": "^0.9.2",
    "minimist": "^1.2.0",
    "ng2lint": "0.0.10",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "source-map-loader": "^0.1.5",
    "tslint": "^3.6.0",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5"
  }
}
