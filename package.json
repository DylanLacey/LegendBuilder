{
  "name": "legend-builder",
  "version": "1.0.0-alpha.7",
  "description": "An advanced League Of Legends champion builder.",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "homepage": "http://legendbuilder.io/",
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist": "rimraf dist",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov/lcov.info",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist && npm run lint",
    "build:dev": "./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist && npm run lint",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached --bail",
    "start": "npm run server",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:prod": "node src/server/httpServer.js",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "static-server": "node src/server/staticServer.js",
    "match-server": "node src/server/matchServer.js",
    "lint": "./node_modules/.bin/tslint src/**/*.ts -e src/typings/**",
    "pree2e": "npm run lint && npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "e2e:live": "protractor --elementExplorer",
    "pretest": "npm run lint",
    "test": "./node_modules/.bin/karma start --single-run",
    "ci": "npm run e2e && npm run test",
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "async": "1.5.2",
    "core-js": "2.2.1",
    "d3": "3.5.16",
    "es6-shim": "0.35.0",
    "fs": "0.0.2",
    "lru-cache": "4.0.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "tinytim": "0.1.1",
    "tracer": "0.8.3",
    "zone.js": "0.6.11"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.17.0-rc.2",
    "compression-webpack-plugin": "^0.3.0",
    "conventional-github-releaser": "^1.1.1",
    "copy-webpack-plugin": "^1.1.1",
    "coveralls": "^2.11.9",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.0.1",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-git": "^1.7.0",
    "gulp-shell": "^0.5.2",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.14.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-matchers-loader": "^0.1.0",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "ng2lint": "^0.0.10",
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.6",
    "protractor": "^3.2.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "source-map-loader": "^0.1.5",
    "tslint": "^3.6.0",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5"
  }
}
