{
  "name": "legend-builder",
  "version": "1.0.0-alpha.8",
  "description": "An advanced League Of Legends champion builder.",
  "bugs": {
    "url": "https://github.com/SteveVanOpstal/LegendBuilder/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Steve Van Opstal",
    "url": "https://github.com/SteveVanOpstal"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SteveVanOpstal/LegendBuilder.git"
  },
  "homepage": "http://legendbuilder.io/",
  "scripts": {
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist",
    "clean:dist:client": "rimraf dist/client",
    "clean:dist:server": "rimraf dist/server",
    "clean:dist:spec": "rimraf dist/spec",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov/lcov.info",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "build": "npm run build:client && npm run build:server",
    "build:client": "npm run build:client:prod",
    "prebuild:client:dev": "npm run clean:dist:client && npm run lint:client",
    "build:client:dev": "./node_modules/.bin/webpack --config config/webpack/client/dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:client:prod": "npm run clean:dist:client && npm run lint:client",
    "build:client:prod": "./node_modules/.bin/webpack --config config/webpack/client/prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "build:server": "npm run build:server:prod",
    "prebuild:server:dev": "npm run clean:dist:server && npm run lint:server",
    "build:server:dev": "./node_modules/.bin/webpack --config config/webpack/server/dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:server:prod": "npm run clean:dist:server && npm run lint:server",
    "build:server:prod": "./node_modules/.bin/webpack --config config/webpack/server/prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "prebuild:server:test": "npm run clean:dist:spec && npm run lint:test",
    "build:server:test": "./node_modules/.bin/webpack --config config/webpack/server/test.js --progress --profile --colors --display-error-details --display-cached",
    "start": "npm run server",
    "server": "npm run server:dev",
    "server:dev": "./node_modules/.bin/webpack-dev-server --config config/webpack/client/dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:prod": "node dist/server/http-server.js",
    "webdriver:update": "./node_modules/.bin/webdriver-manager update",
    "webdriver:start": "./node_modules/.bin/webdriver-manager start",
    "static-server": "npm run build:server:dev && node dist/server/static-server.js",
    "match-server": "npm run build:server:dev && node dist/server/match-server.js",
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "lint:client": "./node_modules/.bin/tslint src/app/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts",
    "lint:server": "./node_modules/.bin/tslint src/server/**/*.ts -e **/*.spec.ts -e **/*.e2e.ts",
    "lint:test": "./node_modules/.bin/tslint src/**/*.spec.ts src/**/testing/**/*.ts",
    "lint:e2e": "./node_modules/.bin/tslint src/**/*.e2e.ts",
    "pree2e": "npm run lint:e2e && npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "e2e:live": "protractor --elementExplorer",
    "pretest": "npm run lint:test",
    "test": "npm run test:client && npm run test:server",
    "test:client": "./node_modules/.bin/karma start",
    "pretest:server": "npm run build:server:test",
    "test:server": "node config/jasmine.js",
    "ci": "npm run e2e && npm run test",
    "typings-install": "./node_modules/.bin/typings install",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "@angular/core": "^2.0.0-rc.1",
    "@angular/compiler": "^2.0.0-rc.1",
    "@angular/common": "^2.0.0-rc.1",
    "@angular/platform-browser": "^2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.1",
    "@angular/http": "^2.0.0-rc.1",
    "@angular/router": "^2.0.0-rc.1",
    "async": "1.5.2",
    "colors": "1.1.2",
    "core-js": "2.3.0",
    "d3": "3.5.17",
    "dateformat": "1.0.12",
    "es6-shim": "0.35.0",
    "fs": "0.0.2",
    "lru-cache": "4.0.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "tinytim": "0.1.1",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^0.17.0-rc.2",
    "compression-webpack-plugin": "^0.3.1",
    "conventional-github-releaser": "^1.1.1",
    "copy-webpack-plugin": "^3.0.0",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "gulp": "^3.9.1",
    "gulp-bump": "^2.0.1",
    "gulp-conventional-changelog": "^1.1.0",
    "gulp-git": "^1.7.0",
    "gulp-shell": "^0.5.2",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.14.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "jasmine-matchers-loader": "^0.1.0",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "ng2lint": "^0.0.10",
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.6",
    "protractor": "^3.2.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^3.8.1",
    "tslint-loader": "^2.1.3",
    "typescript": "^1.8.9",
    "typescript-compiler": "^1.4.1-2",
    "typings": "^1.0.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.12.0"
  }
}
